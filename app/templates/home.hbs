<header>
  <div class="container">
    <h1>Driving Global Goals<br> with Purposeful Brands</h1>
    <h3 class="header-logo">
      <a href=""></a>
    </h3>
    <div class="home-start">
        <a href="/game" id="btn-start" class="home-btn nav-btn">START GAME</a>
    </div>
  </div>
</header>
<div id="site-content">
  <div class="container">
    <div class="wrap">
      <h3 class="colored-logo">
        <a href="/"></a>
      </h3>
      <table>
        <thead>
          <tr>
            <th></th>
            <th>Time</th>
            <th>Name</th>
          </tr>
        </thead>
        <tbody>
        {{#each entries as |entry index|}}
          {{entry-row entry=entry
                      index=index}}
        {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>
